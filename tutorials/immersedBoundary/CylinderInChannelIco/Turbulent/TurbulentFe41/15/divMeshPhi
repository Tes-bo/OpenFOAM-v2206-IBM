FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "15";
    object      divMeshPhi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    ibCylinder
    {
        type            immersedBoundary;
        value           nonuniform 0();
        setDeadValue    false;
        deadValue       0;
    }
    in
    {
        type            zeroGradient;
    }
    out
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


